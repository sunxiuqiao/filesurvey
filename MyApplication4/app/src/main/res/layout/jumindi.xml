<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="要素类型"
            android:layout_weight="1"
            android:layout_marginLeft="10pt"/>
        <EditText
            android:id="@+id/ftName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"
            android:enabled="false"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="属性连接号"
            android:layout_weight="1"
            android:layout_marginLeft="10pt"/>
        <EditText
            android:id="@+id/linkID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/fwcs"
            android:layout_weight="1"
            android:layout_marginLeft="10pt"/>
        <EditText
            android:id="@+id/fwcs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/fscz"
            android:layout_weight="1"
            android:layout_marginLeft="10pt"/>
        <EditText
            android:id="@+id/fwcz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"/>
    </LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/fygz"
        android:layout_weight="1"
        android:layout_marginLeft="10pt"/>
    <EditText
        android:id="@+id/fygz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.4"/>
    </LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/bz"
        android:layout_weight="1"
        android:layout_marginLeft="10pt"/>
    <EditText
        android:id="@+id/bz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.4"/>
</LinearLayout>

</LinearLayout>
